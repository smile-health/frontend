{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "imports": {
    "#components/*": "./src/components/*/index.ts",
    "#components/layouts/*": "./src/components/layouts/*.tsx",
    "#components/modules/*": "./src/components/modules/*.tsx",
    "#components/icons/*": "./src/components/icons/*.tsx",
    "#constants/*": "./src/constants/*.ts",
    "#hooks/*": "./src/hooks/*.ts",
    "#lib/*": "./src/lib/*.ts",
    "#pages/*": "./src/pages/*.tsx",
    "#pages/locales/en/*": "./src/pages/*/locales/en.json",
    "#pages/locales/id/*": "./src/pages/*/locales/id.json",
    "#services/*": "./src/services/*.ts",
    "#shared/*": "./src/shared/*.ts",
    "#store/*": "./src/store/*.ts",
    "#types/*": "./src/types/*.ts",
    "#utils/*": "./src/utils/*.ts",
    "#provider/*": "./src/provider/*.ts",
    "#locales/*": "./src/locales/*.ts"
  },
  "exports": {
    "./components/*": "./src/components/*/index.ts",
    "./components/layouts/*": "./src/components/layouts/*.tsx",
    "./components/modules/*": "./src/components/modules/*.tsx",
    "./components/icons/*": "./src/components/icons/*.tsx",
    "./constants/*": "./src/constants/*.ts",
    "./pages/*": "./src/pages/*.tsx",
    "./provider": "./src/provider/index.tsx",
    "./store/*": "./src/store/*.ts",
    "./styles.css": "./dist/index.css",
    "./hooks/*": "./src/hooks/*.ts",
    "./services/*": "./src/services/*.ts",
    "./utils/*": "./src/utils/*.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "jest --no-coverage",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "peerDependencies": {
    "next": "14.2.30",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
